<div id="parent">

  <aside id="filters">
  {{filters}}
  </aside>

  <div id="items">
  {% for key in keys %}
    {{items[key]}}
  {% endfor %}
  </div>
</div>
<script type="text/javascript">
$('#items > .unpublished').droppable({});
$('[draggable=true]').bind('dragstart', function(ev) {
  ev.originalEvent.dataTransfer.setData('Text', this.innerHTML);
});
</script>
